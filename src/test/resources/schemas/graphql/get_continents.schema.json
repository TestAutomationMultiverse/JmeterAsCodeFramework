{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["continents"],
      "properties": {
        "continents": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["name", "code"],
            "properties": {
              "name": { "type": "string" },
              "code": { "type": "string" }
            }
          }
        }
      }
    }
  }
}